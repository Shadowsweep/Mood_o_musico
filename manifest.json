{
    "manifest_version": 3,
    "name": "AeroMusic",
    "version": "1.0",
    "description": "Get song recommendations based on your mood with ability to add custom songs",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }